var _hyperapp = require("hyperapp");
const RedCard = () => {};
const BlueCard = () => {};
const CardButton = (label, className, clickEvent) => _hyperapp.h("button", {
  type: "button",
  className: `card-button ${className}`,
  onClick: clickEvent
}, [_hyperapp.text(label)]);
_hyperapp.app({
