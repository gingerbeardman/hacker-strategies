var _hyperapp = require("hyperapp");
const RedCard = () => {};
const BlueCard = () => {};
const CardDiv = ({currentCard}) => {
  _hyperapp.h("div", {
    className: "card-div"
  }, [_hyperapp.h("p", {}, _hyperapp.text(currentCard))]);
};
const CardButton = (label, className, clickEvent) => _hyperapp.h("button", {
  type: "button",
  className: `card-button ${className}`,
  onClick: clickEvent
}, [_hyperapp.text(label)]);
_hyperapp.app({
  init: {
    cards: [],
    currentCard: "Choose Red or Blue"
  },
  view: ({cards, currentCard}) => _hyperapp.h("main", {}, [_hyperapp.h("h1", {
    className: "headline"
  }, _hyperapp.text("Hacker Strategies!")), CardButton("Red Team", "red-team"), CardButton("Blue Team", "blue-team")]),
  node: document.getElementById("app")
});
